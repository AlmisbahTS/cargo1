<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cargobooking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CargoBooking">
        <activity android:name=".Admin.EditOrderActivity" />
        <activity android:name=".Superviser.ManageOrderActivity" />
        <activity android:name=".Superviser.SupervisorHomeActivity" />
        <activity android:name=".Admin.AddOrdersActivity" />
        <activity android:name=".Admin.AddUserActivity" />
        <activity android:name=".Admin.AddWHActivity" />
        <activity android:name=".Admin.UsersActivity" />
        <activity android:name=".Admin.ReportActivity" />
        <activity android:name=".Admin.OrderActivity" />
        <activity android:name=".Admin.WarehouseActivity" />
        <activity
            android:name=".Admin.AdminHomeActivity"
            android:label="@string/title_activity_admin_home"
            android:theme="@style/Theme.CargoBooking.NoActionBar" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>